{
  "author": "EricLP, ViddeM",
  "name": "Be productive you fuck!",
  "description": "An extension to help you be more productive",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["js/checkForBadUrls.js"]
  },
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "128": "images/img128.png"
    }
  },
  "icons": {
    "128": "images/img128.png"
  },
  "permissions": ["activeTab", "tabs", "storage", "<all_urls>"],
  "web_accessible_resources": ["insults.json"]
}
